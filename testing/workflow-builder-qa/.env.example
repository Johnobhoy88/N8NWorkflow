# Environment Configuration for Test Suite
# Copy this file to .env and fill in your values

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# n8n Configuration
N8N_WEBHOOK_URL=http://localhost:5678/webhook/workflow-builder
N8N_API_KEY=your_n8n_api_key_here
N8N_BASE_URL=http://localhost:5678

# Gmail Configuration (for E2E tests)
GMAIL_OAUTH2_CLIENT_ID=your_client_id_here
GMAIL_OAUTH2_CLIENT_SECRET=your_client_secret_here
GMAIL_TEST_EMAIL=your_test_email@gmail.com

# Test Configuration
TEST_EMAIL=test@example.com
TEST_TIMEOUT=30000
SILENT_TESTS=false

# Load Testing Configuration
LOAD_TEST_DURATION=5m
LOAD_TEST_VUS=50
LOAD_TEST_THRESHOLD_P95=5000

# Coverage Configuration
COVERAGE_THRESHOLD=80

# CI/CD Configuration
CI=false
CI_PARALLEL=true
CI_MAX_WORKERS=4
